{
  "$schema": "https://schema.tauri.app/config/2",
  "productName": "Floating Bubble Demo",
  "version": "0.1.0",
  "identifier": "com.example.floatingbubbledemo",
  "build": {
    "beforeDevCommand": "npm run dev",
    "devUrl": "http://localhost:1420",
    "beforeBuildCommand": "npm run build",
    "frontendDist": "../dist"
  },
  "app": {
    "withGlobalTauri": true,
    "windows": [
      {
        "title": "Floating Bubble Demo",
        "width": 400,
        "height": 600
      }
    ],
    "security": {
      "csp": null
    }
  },
  "bundle": {
    "active": true,
    "icon": [
      "icons/32x32.png",
      "icons/128x128.png",
      "icons/128x128@2x.png",
      "icons/icon.png"
    ],
    "android": {
      "minSdkVersion": 26
    }
  }
}
